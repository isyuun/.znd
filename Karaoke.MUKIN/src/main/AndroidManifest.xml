<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kr.keumyoung.karaoke.mukin"
    tools:ignore="GoogleAppIndexingWarning">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon,android:theme">
        <activity
            android:name=".apps._main"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".apps._play"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_play"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".apps.test"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".apps.scroll"
            android:label="@string/title_activity_scroll"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".apps.list"
            android:label="@string/title_activity_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".apps.songs"
            android:label="@string/title_activity_songs"
            android:theme="@style/AppTheme.NoActionBar" />
        <!--<activity-->
        <!--android:name=".coupon.apps._preference"-->
        <!--android:label="@string/title_activity_settings" />-->
        <activity
            android:name="kr.keumyoung.karaoke.mukin.coupon.apps._preference"
            android:label="@string/title_activity_settings">
            <intent-filter>
                <action android:name="Open Preference Activity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>

</manifest>